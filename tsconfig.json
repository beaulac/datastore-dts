{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "lib": [
      "es6"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "typeRoots": [
    "node_modules/@types"
  ]
}
